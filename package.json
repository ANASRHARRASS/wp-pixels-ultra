{
  "name": "wp-pixels-ultra",
  "version": "0.4.1",
  "description": "WordPress plugin scaffold for server-side pixel forwarding and Conversions API (CAPI) integration",
  "main": "wp-pixels-ultra.php",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint assets/ includes/ --ext .js,.jsx",
    "lint:fix": "eslint assets/ includes/ --ext .js,.jsx --fix",
    "format": "prettier --write 'assets/**/*.js' 'includes/**/*.php'",
    "format:check": "prettier --check 'assets/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "pixel",
    "tracking",
    "gtm",
    "meta",
    "tiktok",
    "capi",
    "conversions-api",
    "woocommerce"
  ],
  "author": "Ultra Boilerplate",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/node": "^20.0.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/wp-pixels-ultra.git"
  }
}